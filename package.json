{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier:check": "npm run prettier:check:ts && npm run prettier:check:html && npm run prettier:check:scss",
    "prettier:check:ts": "prettier --check 'src/**/*.ts'",
    "prettier:check:html": "prettier --check 'src/**/*.html'",
    "prettier:check:scss": "prettier --check 'src/**/*.scss'",
    "prettier:write": "npm run prettier:write:ts && npm run prettier:write:html && npm run prettier:write:scss",
    "prettier:write:ts": "prettier --write 'src/**/*.ts'",
    "prettier:write:html": "prettier --write 'src/**/*.html'",
    "prettier:write:scss": "prettier --write 'src/**/*.scss'",
    "html:lint": "markuplint 'dist/**/*.html'",
    "stylelint": "stylelint **/*.scss",
    "stylelint:fix": "stylelint --fix .scss",
    "eslint": "eslint --ext .ts --ignore-path .eslintignore .",
    "eslint:fix": "eslint --fix --ext .ts --ignore-path .gitignore .",
    "lint": "npm run html:lint && npm run stylelint && npm run eslint"
  },
  "browserslist": [
    "> 0.2%, not dead",
    "not IE 11"
  ],
  "devDependencies": {
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs": "^0.0.1-security",
    "markuplint": "^3.7.0",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0",
    "vite": "^4.2.0",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-handlebars": "^1.6.0"
  },
  "dependencies": {
    "@acab/reset.css": "^0.5.3"
  }
}
